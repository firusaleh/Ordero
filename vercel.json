{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "trailingSlash": false,
  "cleanUrls": true,
  "functions": {
    "app/**/*": {
      "maxDuration": 30
    },
    "app/api/cron/pos-sync/route.ts": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/cron/pos-sync",
      "schedule": "0 */6 * * *"
    }
  ],
  "headers": [
    {
      "source": "/dashboard/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=0, stale-while-revalidate=0"
        }
      ]
    }
  ]
}